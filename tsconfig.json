{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "~/*": ["./*"],
      "*": ["./*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@navigation/*": ["navigation/*"],
      "@screens/*": ["screens/*"],
      "@theme/*": ["theme/*"],
      "@constants/*": ["constants/*"],
      "@type/*": ["type/*"],
      "@hooks/*": ["hooks/*"],
      "@utils/*": ["utils/*"],
      "@config": ["config"]
    }
  }
}
